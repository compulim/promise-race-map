{
  "name": "promise-race-map",
  "version": "1.0.1-0",
  "description": "Promise.race on a JavaScript map",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel --out-dir lib --ignore __tests__/**/*,**/*.spec.js,**/*.test.js --source-maps true src/",
    "clean": "npx rimraf lib",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "test": "jest",
    "watch": "npm run build -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/compulim/promise-race-map.git"
  },
  "keywords": [
    "promise",
    "race"
  ],
  "author": "William Wong <compulim@hotmail.com> (http://compulim.info/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/compulim/promise-race-map/issues"
  },
  "homepage": "https://github.com/compulim/promise-race-map#readme",
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "testEnvironment": "node"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "coveralls": "^3.0.5",
    "jest": "^24.8.0",
    "regenerator-runtime": "^0.13.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5"
  }
}
